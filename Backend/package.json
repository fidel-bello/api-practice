    "express": "^4.18.1",
    "husky": "^8.0.1",
    "moment": "^2.29.3",
    "mongoose": "^6.3.8",
    "mongoose-unique-validator": "^3.1.0",
    "supertest": "^6.2.3",
    "winston": "^3.7.2"
  },
